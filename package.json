{
  "name": "@mantine/loaders",
  "description": "A collection of loaders",
  "version": "0.0.27",
  "license": "MIT",
  "sideEffects": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./cjs/index.cjs"
      }
    }
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@mantine/core": "0.0.27",
    "@mantine/hooks": "0.0.27"
  },
  "publishConfig": {
    "access": "public"
  }
}
